<%= form_with url: '/applications', method: :post, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, required: true %>
  <%= f.label :address %>
  <%= f.text_field :address, required: true %>
  <%= f.label :city %>
  <%= f.text_field :city, required: true %>
  <%= f.label :state %>
  <%= f.text_field :state, required: true %>
  <%= f.label :zip %>
  <%= f.number_field :zip, required: true %>
  <%= f.submit 'submit' %>
<% end %>
